#!/bin/bash

funct_spript_nfo(){
cat << EOF
    # Script Name	:
    # Author:
    # Created					:
    # Last Modified:
    # Version					:1.0

    # Modifications:

    # Description		:
EOF
echo -e "\e[1;33m--------------------------------------------------------------------------------\e[0m"
}


